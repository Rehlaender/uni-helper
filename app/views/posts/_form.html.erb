<%= form_for(@post) do |f| %>
  <p>
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.label :school_id %>
    <%= f.text_field :school_id %>
  </p>

  <p>
    <%= f.label :category_id %>
    <%= f.text_field :category_id %>
  </p>

  <p>
    <%= f.label :price %>
    <%= f.text_field :price %>
  </p>

  <p>
    <%= f.label :category_post_id %>
    <%= f.text_field :category_post_id %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
